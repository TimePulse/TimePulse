- set_headline  "Listing Projects"
/ **** tabbed table here

= tabbed_panel do |tp|
  = tp.add_tab("Active") do
    %table.listing
      = render :partial => "project_header"
      = list_tree('active_project', 'home/project_list', @root_project.descendants)

  = tp.add_tab("Archived") do
    %table.listing
      = render :partial => "project_header"
      = list_tree('archived_project', 'home/project_list', @root_project.descendants)


= link_to 'New Project', new_project_path
